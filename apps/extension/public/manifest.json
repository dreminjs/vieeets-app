{
  "manifest_version": 3,
  "name": "vieets",
  "version": "1.0",
  "description": "vieets",

  "permissions": [
    "identity",
    "scripting",
    "sidePanel",
    "tabs"
  ],
  "host_permissions": [
    "https://meet.google.com/*"
  ],
 
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {}
}
